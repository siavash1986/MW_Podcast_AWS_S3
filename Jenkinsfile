#!/usr/bin/env groovy
pipeline{
    agent any

    stages{

        stage('build'){
            steps {
                sh 'echo Running build stage!'
            }
        }


        stage('test'){
            steps {
                sh 'echo Running test stage!'
            }
        }

        stage('deploy'){
            steps {
                sh 'echo Running deploy stage1'
            }
        }

    }
}